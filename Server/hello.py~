#! /usr/bin/python
# -*- coding:utf-8 -*-

from flask import Flask
app = Flask(__name__)
app.debug = True
app.secret_key = '123'
app.debug.dump(app)

@app.route('/')
def index():
	return "Holla les deps !"

@app.route('/page1')
def page1():
	return "c'est la page nÂ°1, passe ta route"
	
@app.route('/contact/')
def contact():
	mail = "jean@bon.fr"
	tel = "01 23 45 63 32"
	return "Mail: {} ------- Tel: {}".format(mail, tel)

if __name__ == '__main__':
    app.run()

